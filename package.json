{
    "name": "youtil",
    "version": "3.1.5",
    "description": "油梯，一个与框架、环境无关的小巧、精简、实用的JavaScript工具库",
    "files": [
        "dist"
    ],
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/types/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/types/index.d.ts",
            "require": "./dist/index.cjs",
            "import": "./dist/index.mjs"
        }
    },
    "sideEffects": [
        "es/*"
    ],
    "scripts": {
        "dev": "concurrently \"typedoc --watch\" \"dumi dev\"",
        "_build": "typedoc && ice-pkg build && node after-build.mjs",
        "build": "node esbuild.config.mjs",
        "build:docs": "typedoc && dumi build && node scripts/after-build-doc.mjs",
        "preview": "dumi preview",
        "prepublishOnly": "npm run build",
        "test": "jest",
        "eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./",
        "eslint:fix": "npm run eslint -- --fix",
        "stylelint": "stylelint \"**/*.{css,scss,less}\"",
        "lint": "npm run eslint && npm run stylelint"
    },
    "keywords": [
        "youtil"
    ],
    "dependencies": {
        "@swc/helpers": "0.4.14"
    },
    "devDependencies": {
        "@ice/jsx-runtime": "^0.2.0",
        "@ice/pkg": "1.5.5",
        "@ice/pkg-plugin-docusaurus": "^1.4.18",
        "@iceworks/spec": "^1.0.0",
        "concurrently": "^9.1.2",
        "dumi": "^2.4.21",
        "esbuild": "^0.25.8",
        "esbuild-plugin-d.ts": "^1.3.1",
        "eslint": "^7.0.0",
        "fs-extra": "^11.3.0",
        "jest": "^29.5.0",
        "react": "^16",
        "react-dom": "^16",
        "stylelint": "^13.7.2",
        "ts-jest": "^29.0.5",
        "typedoc": "^0.28.1",
        "typedoc-plugin-markdown": "^4.6.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "github.com/sxei/youtil"
    },
    "author": "sxei",
    "license": "MIT",
    "homepage": "https://youtil.haoji.me"
}
