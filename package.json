{
  "name": "youtil",
  "version": "3.0.0",
  "description": "油梯，一个与框架、环境无关的小巧、精简、实用的JavaScript工具库",
  "files": [
    "es",
    "esm",
    "cjs",
    "docs",
    "umd"
  ],
  "main": "cjs/index.js",
  "module": "es/index.mjs",
  "types": "esm/index.d.ts",
  "exports": {
    ".": {
      "require": "./cjs/index.js",
      "import": "./es/index.mjs",
      "types": "./esm/index.d.ts"
    },
    "./*": {
      "require": "./cjs/*.js",
      "import": "./esm/*.js",
      "types": "./esm/*.d.ts"
    }
  },
  "sideEffects": [
    "es/*"
  ],
  "scripts": {
    "dev": "concurrently \"typedoc --watch\" \"ice-pkg start\"",
    "build": "node before-build.mjs && typedoc && ice-pkg build && node after-build.mjs",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./",
    "eslint:fix": "npm run eslint -- --fix",
    "stylelint": "stylelint \"**/*.{css,scss,less}\"",
    "lint": "npm run eslint && npm run stylelint"
  },
  "keywords": [
    "youtil"
  ],
  "dependencies": {
    "@swc/helpers": "0..1"
  },
  "devDependencies": {
    "@ice/jsx-runtime": "^0.2.0",
    "@ice/pkg": "1.5.5",
    "@ice/pkg-plugin-docusaurus": "^1..18",
    "@iceworks/spec": "^1.0.0",
    "@types/fs-extra": "^11.0.",
    "concurrently": "^9.1.2",
    "eslint": "^7.0.0",
    "fs-extra": "^11.3.0",
    "jest": "^29.5.0",
    "react": "^16",
    "react-dom": "^16",
    "stylelint": "^13.7.2",
    "ts-jest": "^29.0.5",
    "typedoc": "^0.28.1",
    "typedoc-plugin-markdown": "^.6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "github.com/sxei/youtil"
  },
  "author": "sxei",
  "license": "MIT",
  "homepage": "https://youtil.haoji.me"
}